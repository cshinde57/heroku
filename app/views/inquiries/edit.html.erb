<h1>Editing Inquiry</h1>

<%= form_with url: "/inquiries/"+(@inquiry.id).to_s,  scope: :inquiry , method: 'PATCH'  do |form| %>

  <%= form.label :subject %>
  <%= form.text_field :subject %>
  <br>
  <%= form.label :message_content %>
  <%= form.text_field :message_content %>
  <br>
  <br>

  <br>


  <br>
  <%= submit_tag("Update") %>
<% end %>